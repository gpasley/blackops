<!DOCTYPE html>
<html>
<head>
  <title>Blackops</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body class="<%=@image%>">

	<div class="container-fluid">
	            <% flash.each do |name, msg| %>
	                <%= #content_tag(:div, msg, class: "alert alert-#{name}") 
	%>
	            <% end %>
	            <%= yield %>
   </div>
</body>
</html>
